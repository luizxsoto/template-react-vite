{
  "name": "template-react-vite",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "mock": "tsx watch --clear-screen=false --tsconfig tsconfig.app.json ./mock-server/server.ts",
    "lcl": "vite --mode lcl",
    "tst": "vite --mode test",
    "dev": "vite --mode dev",
    "qa": "vite --mode qa",
    "prd": "vite --mode prd",
    "local:mock": "concurrently npm:mock npm:lcl",
    "test:mock": "concurrently npm:mock npm:tst",
    "prebuild": "rimraf dist",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "format": "prettier \"{mock-server,src,tests}/**/*.t{s,sx}\" --check",
    "format:fix": "npm run format -- --write",
    "lint": "eslint \"{mock-server,src,tests}/**/*.t{s,sx}\"",
    "lint:fix": "npm run lint -- --fix",
    "lint:report": "npm run lint -- -c ./tests/.eslintrc.json --format json --output-file reports/lint/result.json",
    "test:e2e:run": "playwright test -c ./tests/playwright/base.config.ts",
    "test:e2e:open": "npm run test:e2e:run -- --ui",
    "test:e2e:performance": "npm run test:e2e:run -- -c ./tests/playwright/performance.config.ts",
    "test:e2e:prepare": "rimraf .nyc_output reports",
    "test:e2e:domain": "npm run test:e2e:run -- -c ./tests/playwright/domain.config.ts",
    "test:e2e:generate-report": "nyc report",
    "pre-push": "concurrently npm:lint npm:format npm:build",
    "check": "ncu",
    "postinstall": "echo \"Checking dependencies...\" & npm run check",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.0",
    "@hookform/resolvers": "4.1.0",
    "@mui/icons-material": "6.4.4",
    "@mui/material": "6.4.4",
    "@tanstack/react-query": "5.64.2",
    "@tanstack/react-query-devtools": "5.64.2",
    "axios": "1.7.9",
    "lodash.get": "^4.4.2",
    "lodash.isequal": "^4.5.0",
    "notistack": "3.0.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "7.54.2",
    "react-idle-timer": "5.7.2",
    "react-number-format": "5.4.3",
    "react-router-dom": "7.1.3",
    "yup": "1.6.1"
  },
  "devDependencies": {
    "@commitlint/cli": "19.6.1",
    "@commitlint/config-conventional": "19.6.0",
    "@eslint/js": "9.19.0",
    "@playwright/test": "1.50.0",
    "@tanstack/eslint-plugin-query": "5.64.2",
    "@types/lodash.get": "^4.4.9",
    "@types/lodash.isequal": "^4.5.8",
    "@types/node": "22.10.10",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "@vitejs/plugin-react-swc": "3.7.2",
    "concurrently": "9.1.2",
    "eslint": "9.19.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-import-resolver-typescript": "3.7.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-react-refresh": "0.4.18",
    "globals": "15.14.0",
    "husky": "9.1.7",
    "lighthouse": "12.3.0",
    "npm-check-updates": "17.1.14",
    "nyc": "17.1.0",
    "path-to-regexp": "8.2.0",
    "playwright": "1.50.0",
    "playwright-lighthouse": "4.0.0",
    "prettier": "3.4.2",
    "rimraf": "6.0.1",
    "tsx": "4.19.2",
    "typescript": "5.7.3",
    "typescript-eslint": "8.21.0",
    "vite": "6.0.11",
    "vite-plugin-istanbul": "6.0.2"
  }
}
